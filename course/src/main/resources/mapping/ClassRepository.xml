<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gloryh.mapper.CourseMapper">
    <insert id="add">
        INSERT INTO course(course_name)
        VALUES (#{name});
    </insert>
    <select id="getList" resultType="com.gloryh.entity.Course">
        SELECT *
        FROM course
    </select>

</mapper>